{
  "author": "Don Peter C. Atunalu",
  "background": {
    "persistent": false,
    "scripts": [
      "background-script.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "description": "RTConn screen sharing is an extension to enable screen sharing using WebRTC. built by Don Peter C. Atunalu",
  "homepage_url": "https://rtconn.tk/",
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "RTConn ScreenSharing",
  "permissions": [
    "desktopCapture"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.8"
}
